{
  "name": "personal-site",
  "version": "4.0.0",
  "homepage": "https://shayne.au/",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/shaynem/personal-site.git"
  },
  "engines": {
    "node": ">=20.x"
  },
  "husky": {
    "hooks": {
      "pre-commit": "find . | grep '.tsx' | grep -vE '.min.js|node_modules|.git|.json|.yarn' | xargs -I file cat file | wc -l > ./src/data/lines.txt"
    }
  },
  "scripts": {
    "analyze": "yarn build && source-map-explorer build/static/js/*.chunk.js",
    "prettier": "prettier --write \"**/*.{js,ts,tsx,css,scss,json,md,mdx,yml}\"",
    "build": "rimraf ./build && craco build",
    "lint": "eslint \"**/*.js\"",
    "predeploy": "yarn build && react-snap",
    "start": "craco start",
    "test": "npx jest"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-brands-svg-icons": "^6.5.1",
    "@fortawesome/free-regular-svg-icons": "^6.5.1",
    "@fortawesome/react-fontawesome": "0.2.0",
    "@yarnpkg/pnpify": "^4.0.1",
    "dayjs": "^1.11.10",
    "markdown-to-jsx": "^7.4.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-burger-menu": "^3.0.9",
    "react-dom": "^18.2.0",
    "react-ga": "^3.3.1",
    "react-helmet-async": "^2.0.4",
    "react-router-dom": "^6.21.1",
    "react-scripts": "^5.0.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.23.3",
    "@babel/preset-env": "^7.23.7",
    "@babel/preset-react": "^7.23.3",
    "@craco/craco": "^7.1.0",
    "@craco/types": "^7.1.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.11",
    "@types/markdown-to-jsx": "^7.0.1",
    "@types/node": "^20.11.0",
    "@types/prop-types": "^15.7.11",
    "@types/react": "^18.2.47",
    "@types/react-burger-menu": "^2.8.7",
    "@types/react-dom": "^18.2.18",
    "@types/react-ga": "^2.3.0",
    "@types/react-helmet-async": "^1.0.3",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "awesome-typescript-loader": "^5.2.1",
    "babel-jest": "^29.7.0",
    "craco-esbuild": "^0.5.2",
    "eslint": "^9.1.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^9.0.10",
    "prettier": "^3.1.1",
    "react-dev-utils": "^12.0.1",
    "react-snap": "^1.23.0",
    "rimraf": "^5.0.5",
    "s": "^1.0.0",
    "sass": "^1.69.7",
    "source-map-explorer": "^2.5.3",
    "source-map-loader": "^5.0.0",
    "typescript": "^5.3.3"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependenciesMeta": {
    "prettier@3.1.1": {
      "unplugged": true
    }
  }
}